<div class='jumbotron'>
  <h1>Quick Notes</h1>
</div>
<div class='container'>
  <% @notes.each do |note|%>
  <div class='notes-container'>
    <h2><%= note.title %></h2>
    <p> <%= note.content %> </p>
    <% note.comments.each do |comment| %>
      <p><%= comment.title %></p>
    <% end %>
  </div>
  <% end %>
</div>
